Credential DEFINITIONS AUTOMATIC TAGS ::= BEGIN
    Credential ::= [PRIVATE 70] SEQUENCE { --Tag ‘FF46’
        tbhCredential TBHCredential,
        hashCode [APPLICATION 1] OCTET STRING
    }

    TBHCredential ::= SEQUENCE {
        eid OCTET STRING,
        certInfos SEQUENCE OF CustomCertInfo,
        encryptedSk OCTET STRING
    }

    CustomCertInfo ::= SEQUENCE {
        ciPkId OCTET STRING,
        ciPk OCTET STRING,
        euiccCrt OCTET STRING,
        eumCrt OCTET STRING
    }
END
